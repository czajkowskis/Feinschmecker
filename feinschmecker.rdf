  <is_vegetarian rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</is_vegetarian>
  <has_link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.bbcgoodfood.com/recipes/the-breakfast-club</has_link>
  <has_image_link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://images.immediate.co.uk/production/volatile/sites/30/2024/04/BreakfastClubSandwich-2f85617.jpg?quality=90&amp;resize=556,505</has_image_link>
</owl:NamedIndividual>

