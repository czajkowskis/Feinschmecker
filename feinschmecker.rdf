<owl:NamedIndividual rdf:about="#spinach,_sweet_potato_&amp;_lentil_dhal">
  <rdf:type rdf:resource="#Recipe"/>
  <has_ingredient rdf:resource="#1_tbsp_sesame_oil"/>
  <has_ingredient rdf:resource="#1_red_onion_finely_chopped"/>
