<owl:NamedIndividual rdf:about="breakfast_hash">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="2_potatoes_cut_into_2cm_cubes_(about_450g)"/>
  <has_ingredient rdf:resource="3_rashers_back_bacon"/>
