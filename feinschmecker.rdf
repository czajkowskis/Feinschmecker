<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="https://jaron.sprute.com/uni/actionable-knowledge-representation/feinschmecker/"
         xmlns="https://jaron.sprute.com/uni/actionable-knowledge-representation/feinschmecker/">

<owl:Ontology rdf:about="https://jaron.sprute.com/uni/actionable-knowledge-representation/feinschmecker">
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This project is about recipes that are used for meal preparations found in the web.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology was made by Jaron Sprute, Bhuvenesh Verma and Szymon Czajkowski.</rdfs:comment>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version: 1.1 - Existing and working ontology with sparse individuals, initial feedback included</owl:versionInfo>
</owl:Ontology>

<owl:ObjectProperty rdf:about="has_ingredient">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="IngredientWithAmount"/>
  <owl:inverseOf rdf:resource="used_for"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to one of the ingredients including their amount required for it.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="authored_by">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Author"/>
  <owl:inverseOf rdf:resource="authored"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to its author.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="requires_time">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Time"/>
  <owl:inverseOf rdf:resource="time_required_by"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to the time required in order to create the in the recipe specified meal.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="is_meal_type">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="MealType"/>
  <owl:inverseOf rdf:resource="meal_type_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to the meal type of which meals can be grouped into.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="has_difficulty">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Difficulty"/>
  <owl:inverseOf rdf:resource="difficulty_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipes to its difficulty.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="has_calories">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Calories"/>
  <owl:inverseOf rdf:resource="calories_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to the amount of calories a meal created according to it approximately has</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="has_protein">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Protein"/>
  <owl:inverseOf rdf:resource="protein_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to the amount of protein a meal created according to it approximately has</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="has_fat">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Fat"/>
  <owl:inverseOf rdf:resource="fat_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to the amount of fat a meal created according to it approximately has</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="has_carbohydrates">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="Carbohydrates"/>
  <owl:inverseOf rdf:resource="carbohydrates_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the recipe to the amount of carbohydrates a meal created according to it approximately has</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="is_ingredient_of">
  <rdfs:domain rdf:resource="Ingredient"/>
  <rdfs:range rdf:resource="IngredientWithAmount"/>
  <owl:inverseOf rdf:resource="type_of_ingredient"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the ingredient to the ingredient with amount which is required for at least one recipe.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="used_for">
  <rdfs:domain rdf:resource="IngredientWithAmount"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="has_ingredient"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the ingredient with amount to the recipe which requires that specific amount of the ingredient.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="type_of_ingredient">
  <rdfs:domain rdf:resource="IngredientWithAmount"/>
  <rdfs:range rdf:resource="Ingredient"/>
  <owl:inverseOf rdf:resource="is_ingredient_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the ingredient with amount to the ingredient it has the amount of.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="authored">
  <rdfs:domain rdf:resource="Author"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="authored_by"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the author to the recipe the author authored.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="is_author_of">
  <rdfs:domain rdf:resource="Author"/>
  <rdfs:range rdf:resource="Source"/>
  <owl:inverseOf rdf:resource="has_author"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the author in form of a user to the source/website he is part of.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="has_author">
  <rdfs:domain rdf:resource="Source"/>
  <rdfs:range rdf:resource="Author"/>
  <owl:inverseOf rdf:resource="is_author_of"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the source/website to the author in form of a user using this website.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="time_required_by">
  <rdfs:domain rdf:resource="Time"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="requires_time"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the time to the recipe which requires the time to complete its meal.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="meal_type_of">
  <rdfs:domain rdf:resource="MealType"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="is_meal_type"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the meal type to a recipe whose meal belongs to it.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="difficulty_of">
  <rdfs:domain rdf:resource="Difficulty"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="has_difficulty"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the difficulty to a recipe having this difficulty.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="calories_of">
  <rdfs:domain rdf:resource="Calories"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="has_calories"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the calories to the recipe whose meal approximately has the amount of calories of.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="protein_of">
  <rdfs:domain rdf:resource="Protein"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="has_protein"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the protein to the recipe whose meal approximately has the amount of protein of.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="fat_of">
  <rdfs:domain rdf:resource="Fat"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="has_fat"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the fat to the recipe whose meal approximately has the amount of fat of.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="carbohydrates_of">
  <rdfs:domain rdf:resource="Carbohydrates"/>
  <rdfs:range rdf:resource="Recipe"/>
  <owl:inverseOf rdf:resource="has_carbohydrates"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A link from the carbohydrates to the recipe whose meal approximately has the amount of carbohydrates of.</rdfs:comment>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="has_name">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A meta class of all descriptions that are identical to their name.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_amount">
  <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A meta class of all descriptions that are identical to their amount.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_recipe_name">
  <rdfs:subPropertyOf rdf:resource="has_name"/>
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the name/title of the recipe.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_instructions">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the instructions one has to follow according to the recipe in form of a text.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="is_vegan">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of whether the recipe is vegan or not.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="is_vegetarian">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of whether the recipe is vegetarian or not.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_link">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the URL the recipe was found on. Since URLs are not supported by owlready2 in form of a text.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_image_link">
  <rdfs:domain rdf:resource="Recipe"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the URL the image of the recipe can be found on. Since URLs are not supported by owlready2 in form of a text.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_ingredient_name">
  <rdfs:subPropertyOf rdf:resource="has_name"/>
  <rdfs:domain rdf:resource="Ingredient"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the name of the ingredient.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_ingredient_with_amount_name">
  <rdfs:subPropertyOf rdf:resource="has_name"/>
  <rdfs:domain rdf:resource="IngredientWithAmount"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the name of the ingredient including its amount and unit.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="amount_of_ingredient">
  <rdfs:subPropertyOf rdf:resource="has_amount"/>
  <rdfs:domain rdf:resource="IngredientWithAmount"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the numeric quantity the ingredient with amount has the ingredient as a floating point number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="unit_of_ingredient">
  <rdfs:domain rdf:resource="IngredientWithAmount"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the unit the ingredient with amount has the ingredient as a text.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_author_name">
  <rdfs:subPropertyOf rdf:resource="has_name"/>
  <rdfs:domain rdf:resource="Author"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the name of the author.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_source_name">
  <rdfs:subPropertyOf rdf:resource="has_name"/>
  <rdfs:domain rdf:resource="Source"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the name of the source/website.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="is_website">
  <rdfs:domain rdf:resource="Source"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the URL the source/website uses. Since URLs are not supported by owlready2 in form of a text.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="amount_of_time">
  <rdfs:subPropertyOf rdf:resource="has_amount"/>
  <rdfs:domain rdf:resource="Time"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the duration of the time in minutes as a number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_meal_type_name">
  <rdfs:subPropertyOf rdf:resource="has_name"/>
  <rdfs:domain rdf:resource="MealType"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the name of the meal type.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="has_numeric_difficulty">
  <rdfs:domain rdf:resource="Difficulty"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the difficulty in form of an int from 1 to 3 according to the difficulties description.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="amount_of_calories">
  <rdfs:subPropertyOf rdf:resource="has_amount"/>
  <rdfs:domain rdf:resource="Calories"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the amount the calories has in kilocalorie as a floating point number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="amount_of_protein">
  <rdfs:subPropertyOf rdf:resource="has_amount"/>
  <rdfs:domain rdf:resource="Protein"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the amount the protein has in gram as a floating point number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="amount_of_fat">
  <rdfs:subPropertyOf rdf:resource="has_amount"/>
  <rdfs:domain rdf:resource="Fat"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the amount the fat has in gram as a floating point number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="amount_of_carbohydrates">
  <rdfs:subPropertyOf rdf:resource="has_amount"/>
  <rdfs:domain rdf:resource="Carbohydrates"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of the amount the carbohydrates has in gram as a floating point number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:Class rdf:about="Recipe">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_recipe_name"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_instructions"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_ingredient"/>
      <owl:someValuesFrom rdf:resource="IngredientWithAmount"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="authored_by"/>
      <owl:onClass rdf:resource="Author"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="requires_time"/>
      <owl:onClass rdf:resource="Time"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="is_vegan"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
      <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="is_vegetarian"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
      <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="is_meal_type"/>
      <owl:onClass rdf:resource="MealType"/>
      <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_difficulty"/>
      <owl:onClass rdf:resource="Difficulty"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_calories"/>
      <owl:onClass rdf:resource="Calories"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_protein"/>
      <owl:onClass rdf:resource="Protein"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_fat"/>
      <owl:onClass rdf:resource="Fat"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_carbohydrates"/>
      <owl:onClass rdf:resource="Carbohydrates"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_link"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A list of steps needed to prepare a meal, along with time of preparation, amount of calories, amounts of macroelements and information about it being vegetarian or vegan</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="Ingredient">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="has_ingredient_name"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
