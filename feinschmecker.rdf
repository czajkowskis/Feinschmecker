  <has_ingredient rdf:resource="#2_tbsp_peanut_butter"/>
  <has_ingredient rdf:resource="#1_small_bunch_fresh__coriander_chopped"/>
  <has_ingredient rdf:resource="#1_cooked__couscous_(or_gluten-free_alternative)"/>
  <authored_by rdf:resource="#miriam_nice"/>
