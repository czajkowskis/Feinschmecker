<owl:NamedIndividual rdf:about="one-pan_piri_piri_chicken_dinner">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="1kg_bone-in_chicken_thighs_and_drumsticks_(equal_quantities_of_each_saves_any_arguments)"/>
