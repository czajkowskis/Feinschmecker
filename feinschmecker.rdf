<owl:NamedIndividual rdf:about="rhubarb_and_custard_fluffy_pancakes">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="175g_self-raising_flour"/>
  <has_ingredient rdf:resource="1_tsp_baking_powder"/>
  <has_ingredient rdf:resource="1_tbsp_golden_caster_sugar"/>
  <has_ingredient rdf:resource="2_tbsp_custard_powder"/>
  <has_ingredient rdf:resource="200ml_milk"/>
  <has_ingredient rdf:resource="3_eggs"/>
  <has_ingredient rdf:resource="2_tbsp_butter"/>
  <has_ingredient rdf:resource="8_tbsp_thick_custard_warmed"/>
  <has_ingredient rdf:resource="4_forced_rhubarb_stalks_trimmed_and_cut_into_4cm_pieces_(about_400g_prepared_weight)"/>
  <has_ingredient rdf:resource="1_medium_orange_juiced"/>
  <has_ingredient rdf:resource="1_vanilla_pod_halved"/>
  <has_ingredient rdf:resource="4_tbsp_caster_sugar"/>
  <authored_by rdf:resource="anna_glover"/>
  <requires_time rdf:resource="time_45"/>
  <is_meal_type rdf:resource="breakfast"/>
  <has_difficulty rdf:resource="difficulty_3"/>
  <has_calories rdf:resource="calories_218.0"/>
  <has_protein rdf:resource="protein_7.0"/>
  <has_fat rdf:resource="fat_6.0"/>
  <has_carbohydrates rdf:resource="carbohydrates_33.0"/>
  <has_recipe_name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rhubarb &amp; custard fluffy pancakes</has_recipe_name>
  <has_instructions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">['step 1Heat the oven to 200C/180C fan/gas 6. Put all the ingredients for the rhubarb in a roasting tin, toss to coat, ensure the rhubarb is in a single layer, cover and roast for 12-14 mins until the rhubarb is soft but still holding its shape. Spoon into a bowl, then pour the roasting juices into a pan and simmer over a medium-high heat for 3-4 mins until syrupy.', 'step 2Whisk the flour, baking powder, sugar and custard powder together in a bowl. Add the milk and eggs, and whisk to a smooth batter. This is easier using a stick blender.', 'step 3Melt a small knob of the butter in a non-stick frying pan over a medium heat, carefully wiping it around the base using kitchen paper. Spoon in some batter to make 8cm circles, and cook for 1-2 mins until bubbles start to appear on the surface. Flip and cook for another 1-2 mins until golden and cooked through. Repeat with the remaining pancake batter and butter.', 'step 4Stack the pancakes up with the custard and poached rhubarb between each layer. Spoon over the rhubarb syrup to serve.']</has_instructions>
  <is_vegan rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</is_vegan>
  <is_vegetarian rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</is_vegetarian>
  <has_link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.bbcgoodfood.com/recipes/rhubarb-custard-fluffy-pancakes</has_link>
  <has_image_link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://images.immediate.co.uk/production/volatile/sites/30/2024/01/Rhubarb-custard-fluffy-pancakes-614afb4.jpg?quality=90&amp;resize=556,505</has_image_link>
