<owl:NamedIndividual rdf:about="#sweet_potato_&amp;_coconut_curry">
  <rdf:type rdf:resource="#Recipe"/>
  <has_ingredient rdf:resource="#4_tbsp_olive_oil"/>
  <has_ingredient rdf:resource="#2_large_onions_halved_and_sliced"/>
