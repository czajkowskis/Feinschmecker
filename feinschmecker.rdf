  <is_vegetarian rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</is_vegetarian>
  <has_link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.bbcgoodfood.com/recipes/manchurian-chicken</has_link>
