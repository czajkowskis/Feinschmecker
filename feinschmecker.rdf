<owl:NamedIndividual rdf:about="chicken_and_ginger_fried_rice">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="500g_boneless,_skinless_chicken_thighs_cut_into_bite-size_pieces"/>
  <has_ingredient rdf:resource="2_tbsp_golden_caster_sugar"/>
  <has_ingredient rdf:resource="3_tbsp_soy_sauce"/>
  <has_ingredient rdf:resource="1_½_tbsp_fish_sauce"/>
  <has_ingredient rdf:resource="3_tbsp_saké"/>
