<owl:NamedIndividual rdf:about="leek_and_kale_hash_with_sage_fried_eggs">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="800g_maris_piper_potatoes_cut_into_large_chunks"/>
