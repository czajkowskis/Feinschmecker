<owl:NamedIndividual rdf:about="meatballs_in_tomato,_cardamom_and_lime_sauce">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="400g_15percent_fat_beef_mince"/>
  <has_ingredient rdf:resource="20g_dill_roughly_chopped"/>
  <has_ingredient rdf:resource="1_½_tsp_bicarbonate_of_soda"/>
  <has_ingredient rdf:resource="3_tbsp_olive_oil"/>
