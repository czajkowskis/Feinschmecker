<owl:NamedIndividual rdf:about="next_level_kedgeree">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="600ml_chicken_stock"/>
  <has_ingredient rdf:resource="300g_smoked_haddock_fillets"/>
  <has_ingredient rdf:resource="25g_butter"/>
