<owl:NamedIndividual rdf:about="teriyaki_and_lime-glazed_chicken_wings">
  <rdf:type rdf:resource="Recipe"/>
  <has_ingredient rdf:resource="2_tbsp_rice_flour"/>
  <has_ingredient rdf:resource="900g_chicken_wings_thoroughly_patted_dry_with_kitchen_paper"/>
