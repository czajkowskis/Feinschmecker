  <has_ingredient rdf:resource="#1½_tsp_ground_cumin"/>
  <has_ingredient rdf:resource="#2_sweet_potatoes_(about_400g/14oz),_cut_into_even_chunks"/>
  <has_ingredient rdf:resource="#250g_red_split_lentils"/>
